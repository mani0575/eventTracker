        <name>GetCallerEmailID</name>
        <script><![CDATA[var GetCallerEmailID = Class.create();
GetCallerEmailID.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getEmail: function() {
        var callId = this.getParameter('caller_sys_id');
        var userGR = new GlideRecord('sys_user');
        
        if (userGR.get(callId)) {
            return userGR.email.toString();
        }
        
        return "";
    },
    
    type: 'GetCallerEmailID'
});]]></script>
