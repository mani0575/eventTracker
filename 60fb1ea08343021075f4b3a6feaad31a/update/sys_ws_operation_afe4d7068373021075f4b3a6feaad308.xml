        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    // implement resource here
	var result = []
	var callerName = request.pathParams.callerID.trim()
	
	var gr = new GlideRecord('incident');
	gr.addQuery("caller_id.name",callerName);
	gr.query();
	result.push({callerName:callerName,totalRecs:gr.getRowCount()})
	var count = 1;
	while(gr.next()){
		result.push({sNo:count++,number:gr.number.toString(),sysId:gr.sys_id.toString()});
	}
	response.setBody(result);

})(request, response);]]></operation_script>
